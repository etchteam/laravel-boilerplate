{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "env": {
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "arrow-body-style": [2, "always"],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  }
}
